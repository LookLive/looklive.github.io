{{#product_occurrences}}
  <div class="media">
    <div class="media-left">
      <a href="#">
        <img class="media-object" src="http:{{product.image.url}}" style="width: 50px; height: 50px;">
      </a>
    </div>
    <div class="media-body">
      <h4 class="media-heading">{{product.brand.name}}</h4>
      {{product.name}}
      {{#details.listings}}
        <a href="{{url}}" target="_blank">{{price}}</a>
      {{/details.listings}}
    </div>
  </div>
{{/product_occurrences}}